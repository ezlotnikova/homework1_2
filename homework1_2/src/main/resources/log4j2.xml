<?xml version="1.0" encoding="UTF-8"?>
<Configuration strict="true">
<Appenders>
    <Console name="STDOUT">
        <PatternLayout>
            <Pattern>%d %p %c{1.} [%t] %m%n</Pattern>
        </PatternLayout>
    </Console>
</Appenders>
<Loggers>
    <Root level="DEBUG">
        <AppenderRef ref="STDOUT"/>
    </Root>
</Loggers>
</Configuration>